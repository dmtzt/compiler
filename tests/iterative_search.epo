komenco()
lokaj variabloj:
    ent arr[5], x, i, n;
    bul found;
{
    n = 5;
    found = malvera;

    presi("Enter 5 values in sorted order:");
    ekde(i = 0; i < n; i = i + 1) {
        arr[i] = legi();
    }

    presi("Enter value to find:");
    x = legi();

    ekde(i = 0; i < n; i = i + 1) {
        se (x == arr[i]) {
            found = vera;
        }
    }

    se (found) {
        presi("Value was found");
    }
    alie {
        presi("Value was not found");
    }
}