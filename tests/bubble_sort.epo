komenco()
lokaj variabloj:
    ent arr[5], i, j, n, temp;
    bul found;
{
    n = 5;
    presi("Enter 5 values in any order:");
    ekde(i = 0; i < n; i = i + 1) {
        arr[i] = legi();
    }

    ekde(i = 0; i < n; i = i + 1) {
        ekde(j = 0; j < n - i - 1; j = j + 1) {
            se (arr[j] > arr[j + 1]) {
                temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }

    presi("Sorted array:");
    ekde(i = 0; i < n; i = i + 1) {
        presi(arr[i]);
    }
}